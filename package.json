{
	"name": "subresources",
	"version": "3.0.0",
	"repository": "git@github.com:sidvishnoi/subresources.git",
	"license": "MIT",
	"author": "Sid Vishnoi <sidvishnoi8@gmail.com> (https://sidvishnoi.com)",
	"main": "index.js",
	"types": "index.d.ts",
	"scripts": {
		"prepare": "tsc -p tsconfig.json",
		"dev": "tsc -p tsconfig.json --watch"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/dom.d.ts",
			"import": "./dist/dom.js"
		},
		"./dom": {
			"types": "./dist/dom.d.ts",
			"import": "./dist/dom.js"
		},
		"./network": {
			"types": "./dist/network.d.ts",
			"import": "./dist/network.js"
		}
	},
	"type": "module",
	"dependencies": {
		"puppeteer": "^24"
	},
	"packageManager": "pnpm@10.30.2",
	"devDependencies": {
		"@types/node": "^24.10.13",
		"typescript": "^5.9"
	}
}
